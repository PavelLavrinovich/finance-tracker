<% if @stock %>
  <div class="well results-block col-md-6">
    <div>
      <p>
        Ticker: <%= @stock.ticker %>
      </p>
      <p>
        Name: <%= @stock.name %>
      </p>
      <p>
        Price: <%= @stock.price %>
      </p>
    </div>
    <div>
      <%= link_to 'Track', user_stocks_path(user: current_user, stock_ticker: @stock.ticker), class: 'btn btn-primary' %>
    </div>
  </div>
<% end %>